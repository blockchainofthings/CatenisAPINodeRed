{
  "name": "node-red-contrib-catenis",
  "version": "1.0.1",
  "description": "Node-RED components to interact with the Bitcoin blockchain by means of the Catenis Enterprise API",
  "homepage": "https://github.com/blockchainofthings/CatenisAPINodeRed",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
